              "documentation/authentication"
            ]
          },
          {
            "group": "Core Concepts",
            "pages": [
              "documentation/parse",
              "documentation/extract",
              "documentation/split",
              "documentation/edit",
              "documentation/pipelines"
            ]
          },
          {
            "group": "Configuration",
            "pages": [
              "documentation/async-processing",
              "documentation/file-handling",
              "documentation/response-format"
            ]
          },
          {
            "group": "Reference",
            "pages": [
              "documentation/credit-usage",
              "documentation/error-codes",
              "documentation/rate-limits"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "openapi": "/api-reference/openapi.json",
        "groups": [
          {
            "group": "API Reference",
            "pages": [
              "api-reference/introduction"
            ]
          },
          {
            "group": "Document Processing",
            "pages": [
              "POST /parse",
              "POST /parse_async",
              "POST /extract",
              "POST /extract_async",
              "POST /split",
              "POST /split_async",
              "POST /edit",
              "POST /edit_async"
            ]
          },
          {
            "group": "Pipelines",
            "pages": [
              "POST /pipeline",
              "POST /pipeline_async"
            ]
          },
          {
            "group": "Files & Jobs",
            "pages": [
              "POST /upload",
              "GET /job/{job_id}",
              "GET /jobs",
              "POST /cancel/{job_id}"
            ]
          }
        ]
      },
      {
        "tab": "Cookbooks",
        "groups": [
          {
            "group": "Cookbooks",
            "pages": [
              "cookbooks/overview",
              "cookbooks/invoice-processing",
              "cookbooks/contract-analysis",
              "cookbooks/multi-document-pipelines"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Studio",
          "href": "https://studio.aifano.com",
          "icon": "browser"
        },
        {
